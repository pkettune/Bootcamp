#include <unistd.h>

void	ft_putchar(char c);

int	main(int argc, char **argv)
{
	int	index;

	index = 0;
	while (argv[0][index] != '\0' && argc)
	{
		ft_putchar(argv[0][index]);
		i++;
	}
	ft_putchar('\n');
	return (0);
}
